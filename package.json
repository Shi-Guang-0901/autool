{
  "name": "aba",
  "license": "MIT",
  "private": true,
  "version": "0.0.1",
  "author": "hecmay",
  "main": "background.js",
  "description": "A SaaS integration and RPA tool",
  "scripts": {
    "init": "python -m venv pyenv && run-script-os",
    "init:macos": "./pyenv/bin/pip install -r ./backend/requirements.txt",
    "init:windows": ".\\pyenv\\Scripts\\pip install -r .\\backend\\requirements.txt && .\\pyenv\\Scripts\\pip install pywin32",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "clean": "run-script-os",
    "clean:macos": "rm -rf build dist dist_electron pyenv release",
    "clean:windows": "rmdir pyenv /q /s",
    "build-py": "run-script-os",
    "build-py:macos": "source ./pyenv/bin/activate && pyinstaller backend/app.spec",
    "build-py:windows": ".\\pyenv\\Scripts\\activate && pyinstaller backend\\app.spec",
    "lint": "vue-cli-service lint",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@codemirror/language": "^6.3.0",
    "@codemirror/legacy-modes": "^6.2.0",
    "active-win": "^7.7.2",
    "codemirror": "^6.0.1",
    "core-js": "^3.8.3",
    "electron-builder": "^23.0.2",
    "js-yaml": "^4.1.0",
    "uiohook-napi": "^1.4.0",
    "vue": "^3.2.13",
    "vue-codemirror": "^6.1.1",
    "vue-router": "^4.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@types/node-fetch": "^2.6.1",
    "@vicons/tabler": "^0.12.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-standard": "^6.1.0",
    "animate.css": "^4.1.1",
    "axios": "^0.27.2",
    "cheerio": "^1.0.0-rc.11",
    "default-passive-events": "^2.0.0",
    "echarts": "^5.3.3",
    "electron": "^20.0.1",
    "electron-devtools-installer": "^3.1.0",
    "electron-store": "^8.0.1",
    "naive-ui": "^2.29.0",
    "pinia": "^2.0.14",
    "run-script-os": "^1.1.6",
    "vue-cli-plugin-electron-builder": "~2.1.1",
    "vue-echarts": "^6.1.0"
  },
  "overrides": {
    "vue-cli-plugin-electron-builder": {
      "electron-builder": "^23.0.2"
    }
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "@vue/standard"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
